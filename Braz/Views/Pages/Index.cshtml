@{
    List<Braz.Models.Post> list = ((List<Braz.Models.Post>)ViewData["Posts"]).OrderByDescending(x=>x.Date).ToList();
}
<section class="blog_s" id="blog_s">
    <div class="title_block">
        <h2>
            Блог
        </h2>
        <div class="line"></div>
    </div>
    <div class="flex_container">
        @for (int i = 0; i < 3; i++)
        {
            <div class="blog_item" style="background-image: url('../../Content/img/news/newsmore/@(list[i].Id.ToString()).index.jpg'); background-size: cover;" aos="zoom-out-left">
                <div class="cont_block">
                    <h4>@list[i].Header</h4>
                    @{ 
                System.Globalization.CultureInfo cult = new System.Globalization.CultureInfo("ru-RU");
            }
            <p>@list[i].Date.ToString("d MMM yyyy",cult)</p>
                    <div class="line"></div>
                    <p>
                        @Html.Raw(list[i].Text.Substring(0, System.Math.Min(list[i].Text.Length, 150)) + "...")
                    </p>
                </div>
                <div class="more_block">
                    <!--<p><i class="fa fa-heart"></i> <span>30</span> likes</p>-->
                    <br><br><br><br><br><br>
                    <a href="/blog/news?post=@(list[i].Id.ToString())">Подробнее</a>
                    <div class="border"></div>
                </div>
            </div>
        }
    </div>
</section>
